package MirageCompiler/Tokenizer

enum TokenizerExceptionType
BadCharInName =         "{0}:{1} bad character when parsing name: {2}"
BadCharInNumber =       "{0}:{1} bad character when parsing number: {2}"
BadCharInSymbol =       "{0}:{1} bad character when parsing symbol: {2}"
StringNotTerminated =   "{0}:{1} string not terminated"
UnexpectedDot =         "{0}:{1} bad character after number with dot: {2}"
UnknownSymbol =         "{0}:{1} unknown symbol found: {2}"
